      #trabalhe .vale-alimentacao h3 {
        transform: translateY(-60px) translateX(-20px) !important;
        margin-top: -10px !important;
      }
      
      /* Regra com máxima especificidade para garantir que funcione */
      body #trabalhe .vale-alimentacao h3 {
        transform: translateY(-60px) translateX(-20px) !important;
        margin-top: -10px !important;
      }
      
      /* Regra com máxima especificidade usando html body */
      html body #trabalhe .vale-alimentacao h3 {
        transform: translateY(-60px) translateX(-20px) !important;
        margin-top: -10px !important;
      }
      
      /* Mover todos os textos dos benefícios em -20px para a esquerda */
      #trabalhe .trabalhe-grid h3 {
        transform: translateX(-20px) !important;
      }
      
      /* Regra com máxima especificidade para todos os benefícios */
      body #trabalhe .trabalhe-grid h3 {
        transform: translateX(-20px) !important;
      }
      
      /* Regra com máxima especificidade usando html body para todos os benefícios */
      html body #trabalhe .trabalhe-grid h3 {
        transform: translateX(-20px) !important;
      }
      
      /* Regra com máxima especificidade e !important para forçar o movimento */
      html body #trabalhe .trabalhe-grid h3[style*="text-align"] {
        transform: translateX(-20px) !important;
      }
      
      /* Regra com máxima especificidade para sobrescrever qualquer regra conflitante */
      html body #trabalhe .trabalhe-grid h3[style*="grid-column"] {
        transform: translateX(-20px) !important;
      }
      
      /* Regra com máxima especificidade para sobrescrever qualquer regra conflitante */
      html body #trabalhe .trabalhe-grid h3[style*="color"] {
        transform: translateX(-20px) !important;
      }
    }
  }
  
